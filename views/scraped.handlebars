  <div class="jumbotron jumbotron-fluid shadow p-5 mb-2 rounded">
    <div class="row">
      <div class="col-lg-12">
        <div class="page-header">
          <h1 class="display-4">Scraper!</h1>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12 shadow p-4 mb-3 bg-white rounded articlelist">
      <h3>Available Articles</h3>
      <div class="list-group list-group-flush">
        {{#each articles}}
        {{#if this.title}}
        <li class="list-group-item cardList">
          <div class="card">
            <div class="card-header">
              <h5 class="card-title">{{this.title}}</h5>
            </div>
            <div class="card-body">
              <p class="card-text">{{this.link}}</p>
              <button data-title="{{this.title}}" data-link="{{this.link}}" class="savebutton btn btn-outline-primary float-right">Save
                Article</button>
            </div>
          </div>

        </li>
        {{/if}}
        {{/each}}
      </div>
    </div>
</div>
